  "bugs": {
    "url": "https://github.com/yelloxing/core.js/issues"
  },
  "homepage": "https://github.com/yelloxing/core.js#readme",
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "cuf": "^1.1.3",
    "rollup": "^1.20.0",
    "uglify-js": "^3.6.0"
  }
