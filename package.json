{
  "name": "@yelloxing/core.js",
  "version": "0.4.1",
  "description": "üê† A modern JavaScript utility library delivering modularity, performance, & extras.",
  "main": "dist/core.min.js",
  "module": "dist/core.min.js",
  "sideEffects": false,
  "scripts": {
    "start": "npm install && npm run build",
    "build": "npm run clean && npm run build:bundle && npm run build:babel && npm run build:uglify && node ./scripts/prependBanner.js",
    "build:bundle": "rollup core.js --file dist/core.es5+.js --format iife --name 'core.js'",
    "build:babel": "npx babel dist/core.es5+.js --out-file dist/core.js",
    "build:uglify": "uglifyjs dist/core.js -o dist/core.min.js",
    "clean": "cuf -d ./dist",
    "postinstall": "node ./scripts/postinstall.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yelloxing/core.js.git"
  },
  "keywords": [
    "core",
    "es",
    "render"
  ],
